xullas bobur endi {
  ismi: "Bobur",
  klichka: "seriy",
  vazni: 69,
  qurollar: {
    soni: 34
  }
};

fn odamTogrisida(odam){
  korsat("===== ODAM INFO ====");
  korsat(odam.ismi, odam.klichka);
  korsat(odam.vazni, "(kg)");
  korsat("Qurollar soni: ", odam.qurollar.soni);
}

odamTogrisida(bobur);